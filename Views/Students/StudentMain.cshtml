
@model Project.Models.ViewModel


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentMain</title>
</head>

<body>
    <h1 align="center">Student Portal</h1>
    <div>
        Student name: @Session["name"]
        <br />

        Courses:
        <br />
        <table>
            <tr>
                <td>Course Name</td>
                <td>Course id</td>
                <td>Day</td>
                <td>Start Time</td>
                <td>End Time</td>
                <td>Moed A</td>
                <td>Moed B</td>


            </tr>
            @foreach (Project.Models.Course obj in Model.ListCource)
            {
                <tr>
                    <td>@obj.name</td>
                    <td>@obj.course_id</td>
                    <td>@obj.day</td>
                    <td>@obj.start_time</td>
                    <td>@obj.end_time</td>
                    <td>@obj.moed1</td>
                    <td>@obj.moed2</td>

                </tr>
            }
        </table>
        <br />
        <table>
            <tr>
                <td>Course ID</td>
                <td>Moed A grade</td>
                <td>Moed B grade</td>
            </tr>
            @foreach (Project.Models.UsersCourses obj in Model.List_users_courses)
            {
                <tr>
                    <td>@obj.course</td>
                    <td>@obj.moed1</td>
                    <td>@obj.moed2</td>

                </tr>
            }
        </table>
        <br />


    </div>
    </body>
</html>
